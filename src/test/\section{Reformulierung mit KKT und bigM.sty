\subsection{Herleitung der KKT-Bedingungen aus der Lagrange-Funktion des Lower-Level-Problems}

\label{problem::Lower_Level_problem::KKT}
\begin{align*}
(5.1) & \quad - \alpha_u^{V} l_u - \eta_u^- + \eta_u^+ = 0, &&\forall u \in V_+ \\
(5.2) & \quad l_u + \alpha_u^{V} l_u  - \mu_u^- + \mu_u^+ = 0, &&\forall u \in V_- \\
(5.3) & \quad -\alpha_l^{V} + \alpha_k^{V} - \delta_a^- + \delta_a^+ + \epsilon_a^- \hat{\Lambda}_a - \epsilon_a^+ \hat{\Lambda}_a = 0, &&\forall a=(l,k) \in A: l \in V,~ k \in V,~ l \neq k \\
(5.4) & \quad \sum_{a \in \delta^{out}(u)} (\epsilon_a^+ - \epsilon_a^-) - \sum_{a \in \delta^{in}(u)} (\epsilon_a^+ - \epsilon_a^-) + \zeta_u^+ - \zeta_u^- = 0, &&\forall u \in V \\
(5.5) & \quad \delta^+, \delta^-, \epsilon^+, \epsilon^-, \zeta^+, \zeta^-, \mu^+, \mu^-, \eta^+, \eta^- \geq 0 \\
(5.6) & \quad \delta_a^- \left( q_a - (1-x_a)q_a^- \right) = 0, &&\forall a \in A \\
(5.7) & \quad \delta_a^+ \left( (1-x_a)q_a^+ - q_a \right) = 0, &&\forall a \in A \\
(5.8) & \quad \epsilon_a^- \left( (\pi_l - \pi_k - \hat{\Lambda}_a q_a) - x_a M_a^- \right) = 0, &&\forall a = (l,k) \in A \\
(5.9) & \quad \epsilon_a^+ \left( x_a M_a^+ - (\pi_l - \pi_k - \hat{\Lambda}_a q_a) \right) = 0, &&\forall a = (l,k) \in A \\
(5.10) & \quad \zeta_u^- (\pi_u - \pi_u^-) = 0, &&\forall u \in V \\
(5.11) & \quad \zeta_u^+ (\pi_u^+ - \pi_u) = 0, &&\forall u \in V \\
(5.12) & \quad \mu_u^- \lambda_u = 0, &&\forall u \in V_- \\
(5.13) & \quad \mu_u^+ (1 - \lambda_u) = 0, &&\forall u \in V_- \\
(5.14) & \quad \eta_u^- (\lambda_u - \lambda_u^-) = 0, &&\forall u \in V_+ \\
(5.15) & \quad \eta_u^+ (1 - \lambda_u) = 0, &&\forall u \in V_+ \\
(5.16) & \quad \sum_{a \in \delta^{out}(u)} q_a - \sum_{a \in \delta^{in}(u)} q_a = (1-\lambda_u)l_u, &&\forall u \in V_+ \\
(5.17) & \quad \sum_{a \in \delta^{out}(u)} q_a - \sum_{a \in \delta^{in}(u)} q_a = -(1-\lambda_u)l_u, &&\forall u \in V_- \\
(5.18) & \quad \sum_{a \in \delta^{out}(u)} q_a - \sum_{a \in \delta^{in}(u)} q_a = 0, &&\forall u \in V_0 := V \setminus (V_+ \cup V_-) \\
(5.19) & \quad x_a M_a^- \leq \pi_l - \pi_k - \hat{\Lambda}_a q_a \leq x_a M_a^+ , &&\forall a=(l,k) \in A \\
(5.20) & \quad \pi_u^- \leq \pi_u \leq \pi_u^+, &&\forall u \in V \\
(5.21) & \quad \lambda_u^-\leq \lambda_u \leq 1, &&\forall u \in V_+\\
(5.22) & \quad 0 \leq \lambda_u \leq 1, &&\forall u \in V_-\\
(5.23) & \quad (1-x_a)q_a^- \leq q_a \leq (1-x_a)q_a^+, &&\forall a \in A
\end{align*}

\section{Reformulierung mit KKT und bigM}


\begin{align*}
    \max_{x} \quad \sum_{u \in V_-} \lambda_u l_u&& \quad\\
    s.t. \quad x \in X \subset \{0,1\}^A,\\
    \quad \quad (\lambda, q , \pi) \in S(x),
\end{align*}

Wobei $S(x)$ die $x-$parametrisierte Optimallösungsmenge des Lower-Level-Problems 

\begin{align*}
\min_{\lambda, q, \pi} \sum_{u \in V_-} \lambda_u l_u \\
\sum_{a \in \delta^{out}(v)} q_a - \sum_{a \in \delta^{in}(v)} q_a &= (1-\lambda_v)l_v, &&\forall v \in V_+ \\
\sum_{a \in \delta^{out}(v)} q_a - \sum_{a \in \delta^{in}(v)} q_a &= -(1-\lambda_v)l_v, &&\forall v \in V_-, &[\alpha_v^{V}] \\
\sum_{a \in \delta^{out}(v)} q_a - \sum_{a \in \delta^{in}(v)} q_a &= 0, &&\forall v \in V_0 := V \setminus (V_+ \cup V_-) \\
(1-x_a)q_a^- &\leq q_a \leq (1-x_a)q_a^+, &&\forall a \in A, &[\delta_a^\pm] \\
x_a M_a^- &\leq \pi_u - \pi_v - \hat{\Lambda}_a q_a \leq x_a M_a^+ , &&\forall a=(u,v) \in A, &[\epsilon_a^\pm] \\
\pi_u^- &\leq \pi_u \leq \pi_u^+, &&\forall u \in V, &[\zeta_u^\pm] \\
0 &\leq \lambda_u \leq 1, &&\forall u \in V_-, &[\mu_u^\pm] \\
\lambda_u^- &\leq \lambda_u \leq 1, &&\forall u \in V_+, &[\eta_u^\pm]
\end{align*}
